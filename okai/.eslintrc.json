{
  "plugins": [
    "@stylistic"
  ],
  "extends": [
    "next/core-web-vitals",
    "plugin:@stylistic/recommended-extends"
  ],
  "rules": {
    "@stylistic/indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "@stylistic/no-tabs": "error",
    "@stylistic/member-delimiter-style": [
      "error",
      {
        "multiline": {
          "delimiter": "semi",
          "requireLast": true
        },
        "singleline": {
          "delimiter": "semi",
          "requireLast": false
        }
      }
    ],
    "@stylistic/eol-last": [
      "error",
      "always"
    ],
    "@stylistic/multiline-ternary": "off",
    "@stylistic/semi": [
      "error",
      "always"
    ],
    "@stylistic/quotes": "off",
    "@stylistic/comma-dangle": "off",
    "@stylistic/brace-style": [
      "error",
      "1tbs"
    ]
  }
}